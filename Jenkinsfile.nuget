pipeline {
    agent any   
        stages {
            stage('Pack') {
                steps {
                    // Pack your project into a NuGet package
                    sh'''
                    mkdir -p Nuget_stage
                    dotnet pack Package.csproj -o Nuget_stage --configfile NuGet.Config /p:Version=1.2.3
                    '''
                }
            }
    }
}
